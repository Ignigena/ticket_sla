{
  "name" : "SLA Remaining",
  "version" : "0.9.8",
  "description" : "Shows SLA remaining on current ticket list.",
  "icons" :
  {
    "48" : "slatoolbar.png"
  },
  "browser_action" :
  {
    "default_icon" : "toolbar.png",
    "default_title" : "Acquia SLA",
    "default_popup": "sla.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "notifications"
  ],
  "background" : {
    "scripts": ["scripts/background.js", "scripts/tool_actions.js"]
  },
  "content_scripts" : [
    {
      "matches" : [
        "https://s5.parature.com/ics/tt/ticket*",
        "https://s5.parature.com/ics/tt/filters.asp"
      ],
      "js" : [
        "scripts/external/jquery-2.0.3.min.js", "scripts/external/moment.min.js", "scripts/external/jquery.timeago.js", "scripts/external/stupidtable.min.js",
        "scripts/date_utilities.js", "scripts/contentscript.js"
      ],
      "css" : ["contentstyle.css"],
      "run_at" : "document_idle",
      "all_frames" : true
    }
  ],
  "web_accessible_resources" : ["scripts/external/jquery-2.0.3.min.map"],
  "options_page": "options.html",
  "update_url": "https://raw.github.com/Ignigena/ticket_sla/master/updates.xml",
  "manifest_version": 2
}