{
  "name" : "SLA Remaining",
  "version" : "0.9",
  "description" : "Shows SLA remaining on current ticket list.",
  "icons" :
  {
    "48" : "slatoolbar.png"
  },
  "browser_action" :
  {
    "default_icon" : "slatoolbar.png",
    "default_title" : "Acquia SLA",
    "default_popup": "sla.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "notifications"
  ],
  "background" : {
    "scripts": ["background.js"]
  },
  "content_scripts" : [
    {
      "matches" : [
        "https://s5.parature.com/ics/tt/ticket*"
      ],
      "js" : ["jquery-2.0.3.min.js", "moment.min.js", "jquery.timeago.js", "contentscript.js"],
      "css" : ["contentstyle.css"],
      "run_at" : "document_idle",
      "all_frames" : true
    }
  ],
  "web_accessible_resources" : ["jquery-2.0.3.min.map"],
  "options_page": "options.html",
  "update_url": "https://raw.github.com/Ignigena/ticket_sla/master/updates.xml",
  "manifest_version": 2
}