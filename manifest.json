{
  "name" : "SLA Remaining",
  "version" : "0.3",
  "description" : "Shows SLA remaining on current ticket list.",
  "background" : {
    "scripts": ["background.js"]
  },
  "browser_action" :
  {
    "default_icon" : "slatoolbar.png",
    "default_title" : "Acquia SLA",
    "default_popup": "sla.html"
  },
  "permissions": ["storage"],
  "content_scripts" : [
    {
      "matches" : [
        "https://s5.parature.com/ics/tt/ticketlist.asp?*"
      ],
      "js" : ["jquery-2.0.0.min.js", "moment.min.js", "jquery.timeago.js", "contentscript.js"],
      "run_at" : "document_idle",
      "all_frames" : true
    }
  ],
  "options_page": "options.html",
  "manifest_version": 2
}