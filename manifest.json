{
  "name" : "SLA Remaining",
  "version" : "0.6.2",
  "description" : "Shows SLA remaining on current ticket list.",
  "browser_action" :
  {
    "default_icon" : "slatoolbar.png",
    "default_title" : "Acquia SLA",
    "default_popup": "sla.html"
  },
  "permissions": ["storage"],
  "content_scripts" : [
    {
      "matches" : [
        "https://s5.parature.com/ics/tt/ticket*"
      ],
      "js" : ["jquery-2.0.0.min.js", "moment.min.js", "jquery.timeago.js", "contentscript.js"],
      "css" : ["contentstyle.css"],
      "run_at" : "document_idle",
      "all_frames" : true
    }
  ],
  "options_page": "options.html",
  "update_url": "https://raw.github.com/Ignigena/ticket_sla/master/updates.xml",
  "manifest_version": 2
}